[bd-mirror-fedora-moduler]
name=Fedora Modular - BD - $releasever - $basearch
baseurl=http://mirror.amberit.com.bd/fedora/linux/releases/$releasever/Modular/$basearch/os/
enabled=1
#metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-$basearch
skip_if_unavailable=False
